{
    "schemaVersion": "1.1",
    "acapPackageConf": {
        "setup": {
            "friendlyName": "lego_acap",
            "appName": "lego_acap",
            "vendor": "Cronvs",
            "embeddedSdkVersion": "3.0",
            "user": {
                "username": "sdk",
                "group": "sdk"
            },
            "runMode": "never",
            "version": "1.0.0"
        },
        "configuration": {
            "paramConfig": [
                {
                    "name": "directory",
                    "default": "https://acme-v02.api.letsencrypt.org/directory",
                    "type": "string"
                },
                {
                    "name": "eab",
                    "default": "",
                    "type": "string"
                },
                {
                    "name": "domain",
                    "default": "example.local",
                    "type": "string"
                },
                {
                    "name": "user",
                    "default": "root",
                    "type": "string"
                },
                {
                    "name": "password",
                    "default": "password",
                    "type": "string"
                }
            ]
        }
    }
}
